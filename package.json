{
  "name": "fancade-editor-scripts",
  "version": "1.0.0",
  "description": "A collection of scripts for Fancade Editor Scripting.",
  "keywords": [
    "fancade",
    "scripts",
    "polyfill"
  ],
  "license": "MIT",
  "author": "Bricked",
  "scripts": {
    "build": "npx tsc --project src",
    "watch": "npx tsc -w --project src",
    "build:scripts": "npx tsc --project scripts",
    "watch:scripts": "npx tsc -w --project scripts",
    "minify": "node scripts/minify -- dist/"
  },
  "devDependencies": {
    "@types/fancade-editor": "^1.12.0",
    "@types/node": "^18.15.5",
    "terser": "^5.16.6",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^10.0.3"
  }
}
